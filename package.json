{
  "name": "vooders.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build-sass": "node-sass --output-style compressed --source-map true node_modules/bulma/bulma.sass public/css/bulma.min.css",
    "install-dev": "./scripts/install-dev.sh && yarn build-sass",
    "start": "yarn build && node ./bin/www",
    "start-dev": "yarn build && nodemon ./bin/www",
    "run-unit-test": "scripts/test-with-coverage.sh 'unittest' 'dist/test/!(acceptance)/**/*Spec.js'",
    "test": "yarn build && yarn run-unit-test"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "bulma": "^0.7.1",
    "cookie-parser": "~1.4.3",
    "express": "~4.16.0",
    "express-handlebars": "^3.0.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/express-handlebars": "^0.0.30",
    "@types/extend": "^3.0.0",
    "@types/http-errors": "^1.6.1",
    "@types/joi": "^13.0.0",
    "@types/mocha": "^2.2.48",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.7",
    "@types/node-sass-middleware": "^0.0.30",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "debug": "~2.6.9",
    "mocha-jenkins-reporter": "^0.4.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "supertest": "^3.1.0",
    "typescript": "^3.0.3",
    "verify-it": "^1.2.1"
  }
}
